weekstable=0;
protocol_state = 3;
}
else if(weekdl==20){
weekmed = 20;
MTX=25;
SSZ=30;
weekdeadline = 24;
weekstable=0;
protocol_state = 4;
}
else if(weekdl==24){
weekmed = 24;
MTX=25;
SSZ=0;
adalimumab=20;
weekdeadline = 36;
weekstable=0;
protocol_state = 5;
}
else if(weekdl==36){
