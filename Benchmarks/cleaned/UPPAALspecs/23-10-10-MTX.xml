}
else if(weekdl==52){
weekmed = 36;
MTX=25;
adalimumab=0;
etanercept=50;
weekdeadline = 52;
weekstable=0;
protocol_state = 7;
}
}

void stableUpdate(){
if (Das28&gt;=2.6){
MTX = MTXold;
SSZ = SSZold;
adalimumab = adalimumabold;
etanercept=etanerceptold;
}
else{
if (etanercept&gt;0){
etanerceptold = etanercept;
etanercept-=2;
}
else if (adalimumab&gt;0){
adalimumabold=adalimumab;
adalimumab-=2;
}
else if (SSZ&gt;0){
SSZold=SSZ;
SSZ-=100;
}
else if (MTX&gt;0){
MTXold=MTX;
MTX-=1;
}
}
