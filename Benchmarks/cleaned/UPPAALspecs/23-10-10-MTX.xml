int Multup = 0;
int Multeq = 3;

double calcdas(){
return ((0.56*sqrt(TenderJoints)+0.28*sqrt(SwollenJoints)+0.70*ln(ESR)+0.014*VAS));
}

double DASmove(int setting)
{
//DAS28 components go up
if (setting == 1){
return (0+MTX*Multup+SSZ*Multup+adalimumab*Multup+etanercept*Multup);
}
else if (setting == 4){
return (0+MTX*Multup+SSZ*Multup+adalimumab*Multup+etanercept*Multup);
}
else if (setting == 7){
return (0+MTX*Multup+SSZ*Multup+adalimumab*Multup+etanercept*Multup);
}
else if (setting == 10){
return (0+MTX*Multup+SSZ*Multup+adalimumab*Multup+etanercept*Multup);
}

//DAS28 components stay equal
else if (setting == 2){
return (30+MTX*Multeq+SSZ*Multeq+adalimumab*Multeq+etanercept*Multeq);
}
else if (setting == 5){
return (30+MTX*Multeq+SSZ*Multeq+adalimumab*Multeq+etanercept*Multeq);
}
else if (setting == 8){
return (30+MTX*Multeq+SSZ*Multeq+adalimumab*Multeq+etanercept*Multeq);
}
else if (setting == 11){
return (30+MTX*Multeq+SSZ*Multeq+adalimumab*Multeq+etanercept*Multeq);
}

//DAS28 components go down
else if (setting == 3){
return (2+MTX*Multdown+SSZ*Multdown+adalimumab*Multdown+etanercept*Multdown);
}
else if (setting == 6){
return (2+MTX*Multdown+SSZ*Multdown+adalimumab*Multdown+etanercept*Multdown);
}
else if (setting == 9){
return (2+MTX*Multdown+SSZ*Multdown+adalimumab*Multdown+etanercept*Multdown);
}
else if (setting == 12){
return (2+MTX*Multdown+SSZ*Multdown+adalimumab*Multdown+etanercept*Multdown);
}
return 1;
}

void DasComponentUpdate(int setting)
{
double tempDas = 1.0;
if (Das28&gt;0 &amp;&amp; Das28&lt;=2){tempDas = 1;}
if (Das28&gt;2 &amp;&amp; Das28&lt;=3){tempDas = 2;}
if (Das28&gt;3 &amp;&amp; Das28&lt;=4){tempDas = 3;}
if (Das28&gt;4){tempDas = 4;}
tempDas = internalDas28;
//Das goes up
if (setting == 1){
randVal = fint(random(4));
if (randVal == 0 &amp;&amp; VAS&lt;100){
VAS=VAS+VAS/60*10*(tempDas/5.5);
if (VAS&gt;100){VAS=100;}
}
else if (randVal==1 &amp;&amp; TenderJoints&lt;28){
TenderJoints = TenderJoints + TenderJoints/22*5*(tempDas/5.5);
if (TenderJoints&gt;28){TenderJoints=28;}
}
else if (randVal ==2 &amp;&amp; SwollenJoints&lt;28){
SwollenJoints = SwollenJoints+SwollenJoints/22*5*(tempDas/5.5);
if (SwollenJoints&gt;28){SwollenJoints=28;}
}
else if (randVal==3 &amp;&amp; ESR&lt; 100){
ESR = ESR+ESR/25*5*(tempDas/5.5);
if (ESR&gt;100){ESR=100;}
}
//if the randomly chosen setting is already at the lowest point:
if (VAS&lt;100){
VAS=VAS+VAS/60*10*(tempDas/5.5);
if (VAS&gt;100){VAS=100;}
}
else if (TenderJoints&lt;28){
TenderJoints = TenderJoints + TenderJoints/22*5*(tempDas/5.5);
if (TenderJoints&gt;28){TenderJoints=28;}
}
else if (SwollenJoints&lt;28){
SwollenJoints = SwollenJoints+SwollenJoints/22*5*(tempDas/5.5);
if (SwollenJoints&gt;28){SwollenJoints=28;}
}
else if (ESR&lt; 100){
ESR = ESR+ESR/25*5*(tempDas/5.5);
if (ESR&gt;100){ESR=100;}
}
}
//Das goes down
else if (setting == 2){
randVal = fint(random(4));
if (randVal == 0 &amp;&amp; VAS&gt;0){
VAS=VAS-VAS/300*(tempDas*tempDas*tempDas);
if (VAS&lt;0){VAS=0;}
}
else if (randVal==1 &amp;&amp; TenderJoints&gt;0){
TenderJoints = TenderJoints - TenderJoints/100*(tempDas*tempDas*tempDas);
if (TenderJoints&lt;0){TenderJoints=0;}
}
else if (randVal ==2 &amp;&amp; SwollenJoints&gt;0){
SwollenJoints = SwollenJoints-SwollenJoints/100*(tempDas*tempDas*tempDas);
if (SwollenJoints&lt;0){SwollenJoints=0;}
}
else if (randVal==3 &amp;&amp; ESR&gt; 1){
ESR = ESR-ESR/250*(tempDas*tempDas*tempDas);
if (ESR&lt;1){ESR=1;}
}
//if the randomly chosen setting is already at the lowest point:
else if ( ESR&gt; 1){
ESR = ESR-ESR/250*(tempDas*tempDas*tempDas);
if (ESR&lt;1){ESR=1;}
}
else if (SwollenJoints&gt;0){
SwollenJoints = SwollenJoints-SwollenJoints/100*(tempDas*tempDas*tempDas);
if (SwollenJoints&lt;0){SwollenJoints=0;}
}
else if (TenderJoints&gt;0){
TenderJoints = TenderJoints - TenderJoints/100*(tempDas*tempDas*tempDas);
if (TenderJoints&lt;0){TenderJoints=0;}
}
else if (VAS&gt;0){
VAS=VAS-VAS/300*(tempDas*tempDas*tempDas);
if (VAS&lt;0){VAS=0;}
}

}
